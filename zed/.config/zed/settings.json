{
  "agent": {
    "default_model": {
      "provider": "anthropic",
      "model": "claude-3-opus-latest"
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "light": "Gruvbox Dark Hard",
    "dark": "One Dark"
  },
  "vim_mode": true,
  "git": {
    "inline_blame": {
      "enabled": false
    },
    "git_gutter": "hide"
  },
  "indent_guides": {
    "enabled": false
  },
  "restore_on_startup": "none",
  "auto_update": false,
  "centered_layout": {
    "left_padding": 0.2,
    "right_padding": 0.2
  },
  "current_line_highlight": "none",
  "cursor_blink": false,
  "cursor_shape": "bar",
  "scrollbar": {
    "show": "never",
    "cursors": true,
    "git_diff": false,
    "search_results": true,
    "selected_symbol": true,
    "diagnostics": "all",
    "axes": {
      "horizontal": true,
      "vertical": true
    }
  },
  "tab_bar": {
    "show": false,
    "show_nav_history_buttons": true
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": true
  },
  "format_on_save": "off",
  "use_autoclose": false,
  "unnecessary_code_fade": 0.8,
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_features": {
    "calt": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 12,
  "file_types": {
    "HTML": ["gohtml"]
  },
  "gutter": {
    "line_numbers": false
  },
  "languages": {
    "Go": {
      "show_edit_predictions": false
    },
    "HTML": {
      "tab_size": 2
    },
    "CSS": {
      "tab_size": 2
    },
    "GDScript": {
      "hard_tabs": true
    }
  },
  "preferred_line_length": 100,
  "title_bar": {
    "show_sign_in": false
  }
}
